/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.dz-btn {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  background: #61a1ff;
  color: white;
  line-height: 80rpx;
  text-align: center;
  border: 0;
  border-radius: 4px;
}

.hidden {
  display: none !important;
}
